//Program on Swapping of 2 numbers using Call by Referenece

#include <stdio.h>
#include <stdlib.h>
void swap(int *x,int *y)
{
    int t;
    t=*x;
    *x=*y;
    *y=t;
    }
    int main()
    {
        int a,b;
        scanf("%d%d",&a,&b);
        printf("Values Befire swapping a=%d b=%d\n",a,b);
        swap(&a,&b);
        printf("Values afetr Swapping a=%d b=%d\n",a,b);
        return 0;
    }
