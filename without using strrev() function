//Program without using strrev() function


#include <stdio.h>
int main() {
          char s1[50],s2[50];
          int i,j=0,n;
          scanf("%s",s1)
;
            for(n=0;s1[n]!='\0';n++);
            for(i=n-1;i>=0;i--)
            {
                s2[j]=s1[i];
                j++;
                
            }
            s2[j]='\0';
            
            printf("%s\n%s",s1,s2);
}


input:
pooja

Output:
pooja
ajoop
